program FindJobs;

uses
  Forms,
  MainMenu in 'MainMenu.pas' {FormMainMenu},
  ToFindPeople in 'ToFindPeople.pas' {FormFindPeople},
  ToCreateBlanket in 'ToCreateBlanket.pas' {FormCreateBlanket},
  ToFindJobs in 'ToFindJobs.pas' {FormFindJobs};

{$R *.res}

begin                                               
  Application.Initialize;                        
  Application.CreateForm(TFormMainMenu, FormMainMenu);
  Application.CreateForm(TFormFindPeople, FormFindPeople);
  Application.CreateForm(TFormCreateBlanket, FormCreateBlanket);
  Application.CreateForm(TFormFindJobs, FormFindJobs);
  Application.Run;
end.                            
